+/*******************************************************************************
+ * Finding associated UIO/physical network interfaces
+ ******************************************************************************/
+static int filter_net_name(const struct dirent *entry)
+{
+	if ((memcmp(entry->d_name, "net:", 4) == 0))
+		return 1;
+	else
+		return 0;
