+typedef struct ICMPV6_ROUTER_ADVERT {
+	ICMPV6_HDR header;
+	u32_t reachable_time;
+	u32_t retransmit_timer;
+} ICMPV6_ROUTER_ADVERT, __FAR__ * pICMPV6_ROUTER_ADVERT;
