+
+	u8_t serverid[4];
+
+	u16_t lease_time[2];
+	u32_t lease_time_nl32;
+	u16_t ipaddr[2];
+	u16_t netmask[2];
+	u16_t dnsaddr[2];
+	u16_t default_router[2];
+
+	time_t last_update;
+};
+
+struct dhcpc_options {
