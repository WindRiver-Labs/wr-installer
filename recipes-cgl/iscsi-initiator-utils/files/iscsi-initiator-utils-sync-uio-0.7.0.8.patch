+	mc_addr = ipv6_context->mc_addr;
+	for (i = 0; i < MAX_MCADDR_TABLE; i++, mc_addr++)
+		if (!memcmp((char __FAR__ *)mc_addr,
+			    (char __FAR__ *)dest_mac->addr, sizeof(MAC_ADDR)))
+			return TRUE;
+	return FALSE;
+}
