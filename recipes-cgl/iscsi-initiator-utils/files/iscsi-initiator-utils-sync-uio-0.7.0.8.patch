+/*---------------------------------------------------------------------------*/
+#ifndef UIP_ARCH_IPCHKSUM
+u16_t uip_ipchksum(struct uip_stack * ustack)
+{
+	u16_t sum;
+	u16_t uip_iph_len;
+
+	if (is_ipv6(ustack))
+		uip_iph_len = UIP_IPv6_H_LEN;
+	else
+		uip_iph_len = UIP_IPv4_H_LEN;
+
+	sum = chksum(0, ustack->network_layer, uip_iph_len);
+	return (sum == 0) ? 0xffff : htons(sum);
+}
+#endif
