+		if (ustack->uip_flags & UIP_ACKDATA) {
+			uip_connr->tcpstateflags = UIP_ESTABLISHED;
+			ustack->uip_flags = UIP_CONNECTED;
+			uip_connr->len = 0;
+			if (ustack->uip_len > 0) {
+				ustack->uip_flags |= UIP_NEWDATA;
+				uip_add_rcv_nxt(ustack, ustack->uip_len);
+			}
+			ustack->uip_slen = 0;
+			UIP_APPCALL(ustack);
