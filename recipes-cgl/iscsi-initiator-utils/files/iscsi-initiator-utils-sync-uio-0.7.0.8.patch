+static void *enable_nic_thread(void *data)
+{
+	nic_t *nic = (nic_t *) data;
+
+	prepare_nic_thread(nic);
+	LOG_INFO(PFX "%s: started NIC enable thread state: 0x%x",
+		 nic->log_name, nic->state)
+
+	    /*  Enable the NIC */
+	    nic_enable(nic);
+
+	pthread_exit(NULL);
+}
