+#define BNX2_RPM_SORT_USER2_BC_EN			(1L<<16)
+#define BNX2_RPM_SORT_USER2_MC_EN			(1L<<17)
+#define BNX2_RPM_SORT_USER2_MC_HSH_EN			(1L<<18)
