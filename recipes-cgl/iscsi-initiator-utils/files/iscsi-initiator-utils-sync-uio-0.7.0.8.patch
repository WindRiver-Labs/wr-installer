+/*---------------------------------------------------------------------------*/
+clock_time_t clock_time(void)
+{
+	struct timeval tv;
+	struct timezone tz;
+
+	gettimeofday(&tv, &tz);
+
+	return tv.tv_sec * 1000 + tv.tv_usec / 1000;
+}
