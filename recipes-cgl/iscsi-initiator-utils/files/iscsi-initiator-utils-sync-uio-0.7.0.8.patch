+#define DRIVER_ISID_0  0x00
+#define DRIVER_ISID_1  0x02
+#define DRIVER_ISID_2  0x3D
+
+/* max len of interface */
+#define ISCSI_MAX_IFACE_LEN	65
+
+#if (ISCSID_VERSION == 872) /* 2.0-872 (RHEL 6.0) */
