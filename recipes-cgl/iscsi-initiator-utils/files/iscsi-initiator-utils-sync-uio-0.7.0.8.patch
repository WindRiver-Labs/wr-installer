+		struct in6_addr v6_addr;
+	} src;
+	union {
+		struct in_addr  v4_addr;
+		struct in6_addr v6_addr;
+	} dst;
+	uint16_t        vlan_id;
+	uint16_t        pmtu;
+}  __attribute__ ((aligned (sizeof(uint64_t))));
